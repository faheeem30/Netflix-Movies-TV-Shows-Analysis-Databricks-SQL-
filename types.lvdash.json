{
  "datasets": [
    {
      "name": "126c16ad",
      "displayName": "Netflix Title Types and Their Counts",
      "queryLines": [
        "SELECT \r\n",
        "  type, \r\n",
        "  COUNT(*) AS total_titles\r\n",
        "FROM netflix_titles\r\n",
        "WHERE type IN ('Movie', 'TV Show')\r\n",
        "GROUP BY type;\r\n"
      ]
    },
    {
      "name": "350e7056",
      "displayName": "Netflix Content Types by Release Year",
      "queryLines": [
        "SELECT \r\n",
        "    release_year,\r\n",
        "    type,\r\n",
        "    COUNT(*) AS total_titles\r\n",
        "FROM netflix_titles\r\n",
        "WHERE release_year IS NOT NULL\r\n",
        "GROUP BY release_year, type\r\n",
        "ORDER BY release_year ASC;\r\n"
      ]
    },
    {
      "name": "31063e4d",
      "displayName": "Top Genres in Netflix Titles by Popularity",
      "queryLines": [
        "WITH genre_split AS (\r\n",
        "    SELECT \r\n",
        "        explode(split(listed_in, ',')) AS genre\r\n",
        "    FROM netflix_titles\r\n",
        "    WHERE listed_in IS NOT NULL\r\n",
        ")\r\n",
        "SELECT \r\n",
        "    trim(genre) AS genre,\r\n",
        "    COUNT(*) AS total_count\r\n",
        "FROM genre_split\r\n",
        "GROUP BY trim(genre)\r\n",
        "ORDER BY total_count DESC\r\n",
        "LIMIT 10;\r\n"
      ]
    }
  ],
